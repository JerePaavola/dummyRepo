pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building branch 1...'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing branch 1...'
            }
        }
        stage('Package') {
            steps {
                echo 'Packaging branch 1...'
            }
        }
    }
}